{
  "expo": {
    "name": "WATCHME",
    "slug": "watchme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/ios_1024.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios_1024.png"
    },
    "android": {
      "icon": "./assets/icons/android_192.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/android_192.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      [
        "expo-video",
        { "supportsBackgroundPlayback": true, "supportsPictureInPicture": true }
      ]
    ]
  }
}
